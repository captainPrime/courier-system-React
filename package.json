    "async": "^3.2.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "nodemon": "^1.19.1",
    "react-image-gallery": "^1.0.7",
    "react-paystack": "^2.0.2",
    "react-redux": "^7.1.0-rc.1",
